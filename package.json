  "scripts": {
    "prepare": "husky install",
    "dev": " vite --open",
    "build:dev": "npm run compile && cross-env VITE_MARKCOIN_BUSINESS_ID=${id} NODE_OPTIONS=--max-old-space-size=16384 --NODE_ENV=production VITE_GIT_COMMIT_ID=$(git rev-parse --short HEAD) vavite --mode dev",
    "build:test": "npm run compile && cross-env VITE_MARKCOIN_BUSINESS_ID=${id} NODE_OPTIONS=--max-old-space-size=16384 --NODE_ENV=production VITE_GIT_COMMIT_ID=$(git rev-parse --short HEAD) vavite --mode test",
    "build:stress": "npm run compile && cross-env VITE_MARKCOIN_BUSINESS_ID=${id} NODE_OPTIONS=--max-old-space-size=16384 --NODE_ENV=production VITE_GIT_COMMIT_ID=$(git rev-parse --short HEAD) vavite --mode stress",
    "build:prod": "npm run compile && cross-env VITE_MARKCOIN_BUSINESS_ID=${id} NODE_OPTIONS=--max-old-space-size=16384 --NODE_ENV=production VITE_GIT_COMMIT_ID=$(git rev-parse --short HEAD) vavite --mode production",
    "build": "vite build",
    "build:run": "vavite",
    "serve": "cross-env NODE_ENV=production node dist/server",
    "compile": "lingui compile",
    "pb": "protoc --ts_out ./src/plugins/ws/protobuf/ts/ --ts_opt long_type_string --proto_path src/plugins/ws/protobuf src/plugins/ws/protobuf/proto/*.proto",
    "ytt": "nbit-ytt --config scripts/ytt/ytt.config.ts",
    "excel": "esno scripts/i18n-update/jsontoexcel",
    "i18ntojson": "esno ./scripts/i18n-update/exceltojson",
    "compareexceltojson": "esno ./scripts/i18n-update/excel-to-json/cn",
    "compareexceltojsonen": "esno ./scripts/i18n-update/excel-to-json/en",
    "i18download": "esno ./scripts/i18n-update/excel-to-json/utils/google-sheet",
    "updatehk": "esno ./scripts/i18n-update/update-hk/index",
    "tsc": "tsc --noEmit --skipLibCheck"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "eslint --fix --quiet",
    "*.css": "stylelint --allow-empty-input --config .stylelintrc.cjs --fix --quiet"
  },
